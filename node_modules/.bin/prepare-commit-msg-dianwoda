#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_prepare-commit-msg-dianwoda@0.2.1@prepare-commit-msg-dianwoda/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../_prepare-commit-msg-dianwoda@0.2.1@prepare-commit-msg-dianwoda/index.js" "$@"
  ret=$?
fi
exit $ret
