#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_json2module@0.0.3@json2module/bin/json2module" "$@"
  ret=$?
else 
  node  "$basedir/../../../_json2module@0.0.3@json2module/bin/json2module" "$@"
  ret=$?
fi
exit $ret
